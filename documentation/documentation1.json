            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "image": {
                    "type": "string"
                  },
                  "country": {
                    "type": "string"
                  },
                  "city": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  },
                  "phone": {
                    "type": "integer"
                  },
                  "required": [
                    "name",
                    "image",
                    "country",
                    "city",
                    "password",
                    "phone"
                  ]
                }
              }
            }
          }
        },
        "responses": {
          "200": {
