                                "example": "123456"
                              },
                              "resetPasswordToken": {
                                "type": "string",
                                "example": "null"
                              },
                              "isActive": {
                                "type": "boolean",
                                "example": true
                              },
                              "createdAt": {
                                "type": "string",
                                "example": "2023-12-12T16:00:39.955Z"
                              },
                              "updatedAt": {
                                "type": "string",
                                "example": "2023-12-19T18:29:35.899Z"
                              }
                            }
