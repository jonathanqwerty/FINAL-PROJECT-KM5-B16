    },
    "/auth/reset-password": {
      "post": {
        "tags": ["Auth"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "success": {
                          "type": "string",
                          "example": "The reset password link has been sent to your email"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/auth/set-password/{token}": {
      "post": {
        "tags": ["Auth"],
        "parameters": [
          {
            "name": "token",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "password": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "succsess",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "success": {
                          "type": "string",
                          "example": "success reset your password"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/auth/google": {
      "post": {
        "tags": ["Auth"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "access_token": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "token": {
                          "type": "string",
                          "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6MSwiZW1haWwiOiJuYXRoYW5hZWxqb25hdG"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/me/profile": {
      "get": {
        "tags": ["profile"],
        "summary": "Get profile",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer",
                            "example": 1
                          },
                          "email": {
                            "type": "string",
                            "example": "jhon@duar.com"
                          },
                          "phone": {
                            "type": "string",
                            "example": "0812345678"
                          },
                          "password": {
                            "type": "string",
                            "example": "$2b$05$wGI5m52hE7/m7z6bOhwMKOUpgK85k7"
                          },
                          "profileId": {
                            "type": "integer",
                            "example": 1
                          },
                          "role": {
                            "type": "string",
                            "example": "user"
                          },
                          "validasi": {
                            "type": "string",
                            "example": "123456"
                          },
                          "resetPasswordToken": {
                            "type": "string",
                            "example": "null"
                          },
                          "isActive": {
                            "type": "boolean",
                            "example": true
                          },
                          "createdAt": {
                            "type": "string",
                            "example": "2023-12-12T16:00:39.955Z"
                          },
                          "updatedAt": {
                            "type": "string",
                            "example": "2023-12-19T18:29:35.899Z"
                          },
                          "profile": {
                            "type": "object",
                            "properties": {
                              "id": {
                                "type": "integer",
                                "example": 1
                              },
                              "name": {
                                "type": "string",
                                "example": "user"
                              },
                              "image": {
                                "type": "string",
                                "example": "klashjdkl"
                              },
                              "country": {
                                "type": "string",
                                "example": "indonesia"
                              },
                              "city": {
                                "type": "string",
                                "example": "jakarta"
                              },
                              "createdAt": {
                                "type": "string",
                                "example": "2023-12-12T16:00:39.955Z"
                              },
                              "updatedAt": {
                                "type": "string",
                                "example": "2023-12-19T18:29:35.899Z"
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    },
